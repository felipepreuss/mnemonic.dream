extends Label

@onready var player = $"../../../.."

func _ready() -> void:
	pass
	
func _physics_process(delta: float) -> void:
	if player.power_up == "pop_candy":
		text = "Pop Candy"
	elif player.power_up == "chiclete":
		text = "Chiclete"
	elif player.power_up == "chocolate":
		text = "Chocolate"
	else:
		text = "None"

